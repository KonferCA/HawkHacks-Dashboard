{
    "name": "dashboard",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"pnpm run vite\" \"pnpm run emulators\"",
        "build": "tsc -b && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
        "preview": "vite preview",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "test": "vitest",
        "test:ci": "vitest run --coverage",
        "typecheck": "tsc --noEmit",
        "emulators": "firebase emulators:start --project spurhacks-dashboard --import ./data",
        "setup": "firebase init emulators",
        "firebase": "firebase",
        "vite": "vite",
        "analyse-build": "vite-bundle-visualizer",
        "dev:withFunctions": "cd functions && tsc && cd .. && pnpm run dev"
    },
    "dependencies": {
        "@chakra-ui/react": "^3.15.0",
        "@emotion/react": "^11.14.0",
        "@headlessui/react": "^1.7.19",
        "@heroicons/react": "^2.2.0",
        "@tanstack/react-query": "^5.72.2",
        "class-variance-authority": "^0.7.1",
        "date-fns": "^3.6.0",
        "firebase": "^11.5.0",
        "hamburger-react": "^2.5.2",
        "nanoid": "^5.1.3",
        "next-themes": "^0.4.6",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.5.0",
        "react-router-dom": "^6.30.0",
        "tailwind-merge": "^1.14.0",
        "zod": "^3.24.3",
        "zustand": "^4.5.6"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.10",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^20.17.24",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/react-transition-group": "^4.4.12",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^0.34.6",
        "autoprefixer": "^10.4.21",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.19",
        "firebase-tools": "^13.34.0",
        "jsdom": "^26.0.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.8.2",
        "vite": "^6.2.5",
        "vite-bundle-visualizer": "^1.2.1",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^3.0.8"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@firebase/util",
            "esbuild",
            "protobufjs",
            "re2"
        ]
    }
}
